	<queryString>
		<![CDATA[SELECT t_invoice.index_no,
	year(t_invoice.invoice_date) AS invoice_date,
	COUNT(t_invoice.index_no) AS job,
	(SELECT ifnull(sum(view_invoice_payment.total),0) FROM view_invoice_payment WHERE year(view_invoice_payment.DATE)=year(t_invoice.invoice_date)
	AND view_invoice_payment.`type`='FACTORY INVOICE' ) AS inv_amount,
	(SELECT ifnull(sum(channel_payment.amount),0) FROM channel_payment WHERE year(channel_payment.DATE)=year(t_invoice.invoice_date)) AS channel_amount,
	(SELECT ifnull(sum(search_voucher.credit),0) FROM search_voucher WHERE year(search_voucher.DATE)=year(t_invoice.invoice_date)
	AND search_voucher.is_expense=1 AND search_voucher.`type`='VOUCHER') AS expense
FROM t_invoice
WHERE t_invoice.invoice_date>= $P{from}  AND t_invoice.invoice_date<= $P{to}   
and t_invoice.status2='FACTORY INVOICE' 
GROUP BY year(t_invoice.invoice_date)]]>
	</queryString>
